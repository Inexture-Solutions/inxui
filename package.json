{
  "name": "inxui-system",
  "author": {
    "name": "Inexture",
    "url": "https://www.inexture.com"
  },
  "private": false,
  "scripts": {
    "build": "turbo build --parallel",
    "dev": "turbo dev --parallel",
    "clean": "turbo run clean && find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "release": "turbo run build && changeset version && changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.5",
    "prettier": "^3.3.3",
    "turbo": "^2.2.3",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=20.18.0"
  }
}
