{
  "name": "inxui-system",
  "author": {
    "name": "Inexture",
    "url": "https://www.inexture.com"
  },
  "private": false,
  "scripts": {
    "build": "turbo build --parallel",
    "dev": "turbo dev --parallel",
    "clean": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "postversion": "git add . && git commit -m \"chore: release v$(jq -r .version < package.json)\" && git push"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "^2.2.3",
    "typescript": "^5.6.3",
    "@changesets/cli": "^2.27.9"
  },
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=20.15.1"
  }
}
